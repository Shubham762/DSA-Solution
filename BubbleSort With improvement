let nums=[-1,0,7,10,-2,5]
function bubbleSort(nums) {
    let n=nums.length-1;
   for(let i=0;i<n;i++){
       let isSwapped=false;
       for(let j=0; j<n-i;j++){
           if(nums[j]>nums[j+1]){
               temp=nums[j];
               nums[j]=nums[j+1];
               nums[j+1]=temp;
               isSwapped=true;
           }
       }
       if(!isSwapped) break;
   }
   return nums;
};

console.log(bubbleSort(nums))
